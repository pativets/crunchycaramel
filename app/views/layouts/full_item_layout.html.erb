<!DOCTYPE html>
<html>
<head>	
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag 'application.js' %>
  <%= javascript_include_tag 'jquery-1.4.4.min.js' %>
  <%= javascript_include_tag 'rails' %>
  <%= javascript_include_tag 'jquery.coda-slider-2.0.js' %>
  <%= javascript_include_tag 'jquery-ui-1.8.10.custom.min.js' %>
  <%= javascript_include_tag 'jquery.easing.1.3.js' %>
  <%= javascript_include_tag 'jquery.lightbox-0.5.js' %>
  <%= javascript_include_tag 'facebox.js' %>
</head>
<body>
	<section class="round">
		<% flash.each do |key, value| %>
            <%= content_tag(:div, value, :class => "flash #{key}") %>
        <% end %>
		<%= yield %>
	</section>
</body>
</html>